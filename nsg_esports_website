<body>

<h1>Target Priority Challenge</h1>
<p>Select the BEST target only</p>

<input id="name" placeholder="Player Name">
<input id="uid" placeholder="Game UID">
<br><br>

<button onclick="startGame()">Start Test</button>

<h2 id="info"></h2>

<div id="arena">
  <div class="target" onclick="hit(0)"></div>
  <div class="target" onclick="hit(1)"></div>
  <div class="target" onclick="hit(2)"></div>
</div>

<h2 id="final"></h2>

<button onclick="submit()">Submit Result</button>

<script>
let score=0,time=40;
let values=[10,5,-5];
let colors=["red","yellow","black"];
let correctIndex=0;
let timer;

const arena=document.getElementById("arena");
const targets=document.querySelectorAll(".target");

function startGame(){
  if(!name.value||!uid.value){
    alert("Enter name & UID");
    return;
  }

  score=0; time=40;
  arena.style.display="flex";
  nextRound();
  update();

  timer=setInterval(()=>{
    time--;
    update();
    if(time<=0) endGame();
  },1000);
}

function nextRound(){
  let order=[0,1,2].sort(()=>Math.random()-0.5);
  correctIndex=order.indexOf(0);

  targets.forEach((t,i)=>{
    t.className="target "+colors[order[i]];
    t.innerText=values[order[i]];
  });
}

function hit(i){
  if(i===correctIndex){
    score+=10;
  }else{
    score=Math.max(0,score-5);
  }
  nextRound();
  update();
}

function update(){
  info.innerText=`Time: ${time}s | Score: ${score}`;
}

function endGame(){
  clearInterval(timer);
  arena.style.display="none";

  final.innerText=`Final Score: ${score}`;
  localStorage.setItem("nsgResult",JSON.stringify({
    name:name.value,
    uid:uid.value,
    score:score
  }));
}

function submit(){
  let d=JSON.parse(localStorage.getItem("nsgResult"));
  if(!d){alert("Play first");return;}

  fetch("https://formspree.io/f/FORM_ID_HERE",{
    method:"POST",
    headers:{'Content-Type':'application/json'},
    body:JSON.stringify(d)
  }).catch(()=>{});

  alert("Result sent to NSG Admin");
}
</script>

</body>
</html>
